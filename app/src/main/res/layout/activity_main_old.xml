<?xml version="1.0" encoding="utf-8"?>





<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.wordpress.farhantanvirutshaw.lostsurvive.MainActivity">

<include
    android:id="@+id/main_activity_appbar"
    layout="@layout/app_bar"/>

<Button
    android:onClick="go"
    android:layout_below="@id/main_activity_appbar"
    android:text="GO"
    android:id="@+id/main_activity_btn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" />

    <Button
        android:onClick="go2"
        android:layout_below="@id/main_activity_btn"
        android:text="GO 2"
        android:id="@+id/main_activity_btn_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/txt"
        android:textSize="16sp"
        android:layout_marginTop="16dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/main_activity_btn_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />








</RelativeLayout>